programa {
  funcao inicio() {
    inteiro num1, num2, num3, num4, diferenca

    escreva("Informe o primeiro número? ")
    leia(num1)
    escreva("\nInforme o segundo número? ")
    leia(num2)
    escreva("\nInforme o terceiro número? ")
    leia(num3)
    escreva("\nInforme o quarto número? ")
    leia(num4)

    diferenca = (num1 * num2) - (num3 * num4)

    escreva("A diferênça entre os números é: " + diferenca)

    
  }
}
